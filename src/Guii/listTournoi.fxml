<?import javafx.scene.text.Text?>

<AnchorPane id="AnchorPane" prefHeight="430.0" prefWidth="987.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Guii.ListTournoiController">
   <children>
      <TableView fx:id="TournoiTable" layoutX="20.0" layoutY="140.0" prefHeight="290.0" prefWidth="925.0" stylesheets="@../Values/style.css">
         <columns>
            <TableColumn fx:id="idcoll" maxWidth="200.0" style="-fx-alignment: center;" text="id" />
            <TableColumn fx:id="vueColl" maxWidth="200.0" prefWidth="200.0" style="-fx-alignment: center;" text="Nom" />
            <TableColumn fx:id="descriptionColl" maxWidth="300.0" prefWidth="201.0" style="-fx-alignment: center;" text="Descriptions" />
            <TableColumn fx:id="nbrColl" prefWidth="226.0" text="Joueur" />
            <TableColumn fx:id="actionsColl" maxWidth="217.0" prefWidth="217.0" style="-fx-alignment: center;" text="Actions" />
         </columns>
      </TableView>
      <Pane layoutX="10.0" layoutY="71.0" prefHeight="3.0" prefWidth="200.0" style="-fx-background-color: black;" />
      <Text fx:id="titre" layoutX="45.0" layoutY="57.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Liste des Tournoi">
         <font>
            <Font name="Baskerville Old Face" size="36.0" />
         </font>
      </Text>
      <Button layoutX="134.0" layoutY="88.0" mnemonicParsing="false" onMouseClicked="#refreshTable" prefHeight="46.0" prefWidth="69.0" styleClass="my-button" stylesheets="@../Values/style.css" textAlignment="CENTER" textFill="#e10808">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
         <graphic>
            <ImageView fx:id="refrechbtn" fitHeight="41.0" fitWidth="38.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../img/refresh.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <Button fx:id="ajoutbtn" layoutX="83.0" layoutY="90.0" mnemonicParsing="false" onMouseClicked="#getAdd" styleClass="my-button" stylesheets="@../Values/style.css" textFill="#d91c1c">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
         <graphic>
            <ImageView fitHeight="59.0" fitWidth="34.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../img/plus.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <Button layoutX="917.0" layoutY="18.0" mnemonicParsing="false" onMouseClicked="#close" styleClass="add-btn" stylesheets="@../Values/style.css" text="x" />
