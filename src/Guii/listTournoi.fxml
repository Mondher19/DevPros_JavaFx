         <font>
            <Font name="System Bold" size="15.0" />
         </font>
         <graphic>
            <ImageView fitHeight="37.0" fitWidth="37.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../img/edit.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <TextField fx:id="tfsearchmember" layoutX="414.0" layoutY="97.0" onKeyPressed="#search" />
      <Button layoutX="687.0" layoutY="75.0" mnemonicParsing="false" onAction="#trierProduit" styleClass="my-button" stylesheets="@../Values/style.css">
         <graphic>
            <ImageView fitHeight="90.0" fitWidth="42.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../img/sort-descending.png" />
               </image>
            </ImageView>
         </graphic></Button>
      <Button fx:id="imprimebtn" layoutX="766.0" layoutY="65.0" mnemonicParsing="false" onAction="#imprimerTable" prefHeight="72.0" prefWidth="73.0" styleClass="my-button" stylesheets="@../Values/style.css">
         <graphic>
            <ImageView fitHeight="62.0" fitWidth="57.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../img/printer.png" />
               </image>
            </ImageView>
         </graphic></Button>
      <Button layoutX="858.0" layoutY="66.0" mnemonicParsing="false" onAction="#stat" styleClass="my-button" stylesheets="@../Values/style.css">
         <graphic>
            <ImageView fitHeight="52.0" fitWidth="74.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../img/statistiques.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <Label layoutX="325.0" layoutY="102.0" text="Recherche" />
   </children>
</AnchorPane>
